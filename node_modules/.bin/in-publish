#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/in-publish/in-publish.js/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/in-publish/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/node_modules:/mnt/c/Users/elper/Desktop/work/node_modules:/mnt/c/Users/elper/Desktop/node_modules:/mnt/c/Users/elper/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../in-publish/in-publish.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/in-publish/in-publish.js/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/in-publish/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/node_modules:/mnt/c/Users/elper/Desktop/work/node_modules:/mnt/c/Users/elper/Desktop/node_modules:/mnt/c/Users/elper/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" node  "$basedir/../in-publish/in-publish.js" "$@"
  ret=$?
fi
exit $ret
