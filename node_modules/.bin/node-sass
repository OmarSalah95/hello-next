#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/.registry.npmjs.org/node-sass/4.12.0/node_modules/node-sass/bin/node-sass/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/.registry.npmjs.org/node-sass/4.12.0/node_modules/node-sass/bin/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/.registry.npmjs.org/node-sass/4.12.0/node_modules/node-sass/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/.registry.npmjs.org/node-sass/4.12.0/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/.registry.npmjs.org/node-sass/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/node_modules:/mnt/c/Users/elper/Desktop/work/node_modules:/mnt/c/Users/elper/Desktop/node_modules:/mnt/c/Users/elper/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/node-sass/bin/node-sass/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/node-sass/bin/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/node-sass/node_modules" "$basedir/node"  "$basedir/../node-sass/bin/node-sass" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/.registry.npmjs.org/node-sass/4.12.0/node_modules/node-sass/bin/node-sass/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/.registry.npmjs.org/node-sass/4.12.0/node_modules/node-sass/bin/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/.registry.npmjs.org/node-sass/4.12.0/node_modules/node-sass/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/.registry.npmjs.org/node-sass/4.12.0/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/.registry.npmjs.org/node-sass/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/node_modules:/mnt/c/Users/elper/Desktop/work/node_modules:/mnt/c/Users/elper/Desktop/node_modules:/mnt/c/Users/elper/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/node-sass/bin/node-sass/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/node-sass/bin/node_modules:/mnt/c/Users/elper/Desktop/work/NextJs/hello-next/node_modules/node-sass/node_modules" node  "$basedir/../node-sass/bin/node-sass" "$@"
